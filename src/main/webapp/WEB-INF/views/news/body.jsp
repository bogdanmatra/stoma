<script>

    $(document).ready(function() {
        var elements=$('.list-group-item');

        elements.first().addClass("active");

        elements.click(function (event) {
            //remove all pre-existing active classes
            $('.active').removeClass('active');
            $(this).addClass('active');
        });
    });

</script>